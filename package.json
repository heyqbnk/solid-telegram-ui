{
  "name": "solid-telegram-ui",
  "version": "0.0.0",
  "author": "Vladislav Kibenko <wolfram.deus@gmail.com>",
  "homepage": "https://github.com/heyqbnk/solid-telegram-ui#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:heyqbnk/solid-telegram-ui.git"
  },
  "bugs": {
    "url": "https://github.com/heyqbnk/solid-telegram-ui/issues"
  },
  "keywords": [
    "telegram",
    "typescript",
    "solid"
  ],
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "browser": "./dist/index.iife.js",
  "module": "./dist/index.mjs",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "pnpm run lint --fix",
    "storybook": "storybook dev -p 6006",
    "build": "vite build",
    "build:icons": "node cli/index.js build-icons && eslint --fix src/icons/index.ts",
    "build:storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-viewport": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/types": "^8.0.4",
    "@tma.js/sdk": "^1.5.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-solid": "^0.13.2",
    "sass": "^1.72.0",
    "solid-js": "^1.8.16",
    "storybook": "^8.0.4",
    "storybook-solidjs": "1.0.0-beta.2",
    "storybook-solidjs-vite": "1.0.0-beta.2",
    "svgo": "^3.2.0",
    "vite": "^5.2.6",
    "vite-plugin-dts": "^3.8.0",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "solid-transition-group": "^0.2.3"
  },
  "peerDependencies": {
    "@tma.js/sdk": "^1.5.1",
    "solid-js": "^1.8.16"
  }
}
